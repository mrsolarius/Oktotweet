

{
  "master_picture": "app/assets/images/oktotweet.svg",
  "favicon_design": {
    "ios": {
      "picture_aspect": "background_and_margin",
      "background_color": "#ffffff",
      "margin": "18%",
      "assets": {
        "ios6_and_prior_icons": true,
        "ios7_and_later_icons": true,
        "precomposed_icons": false,
        "declare_only_default_icon": true
      },
      "app_name": "Oktotweet"
    },
    "desktop_browser": {
      "design": "raw"
    },
    "windows": {
      "picture_aspect": "white_silhouette",
      "background_color": "#fe0134",
      "on_conflict": "override",
      "assets": {
        "windows_80_ie_10_tile": true,
        "windows_10_ie_11_edge_tiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      },
      "app_name": "Oktotweet"
    },
    "android_chrome": {
      "picture_aspect": "shadow",
      "theme_color": "#ffffff",
      "manifest": {
        "name": "Oktotweet",
        "display": "standalone",
        "orientation": "portrait",
        "on_conflict": "override",
        "declared": true
      },
      "assets": {
        "legacy_icon": false,
        "low_resolution_icons": false
      }
    },
    "safari_pinned_tab": {
      "picture_aspect": "silhouette",
      "theme_color": "#fe0134"
    }
  },
  "settings": {
    "compression": 5,
    "scaling_algorithm": "Lanczos",
    "error_on_image_too_small": false,
    "readme_file": true,
    "html_code_file": true,
    "use_path_as_is": false
  }
}

